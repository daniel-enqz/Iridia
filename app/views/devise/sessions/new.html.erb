<div class="sign-in-card">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= f.label :email, class: "input-label" %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "large-field input" %>

    <%= f.label :password, class: "input-label" %>
    <%= f.password_field :password, autocomplete: "current-password", class: "large-field input"  %>

    <div class="actions">
      <%= f.submit "Log in" %>
    </div>
  <% end %>
</div>
